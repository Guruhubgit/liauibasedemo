            </column>  
            <column  name="empfirstnameDemo"  type="varchar(50)"/>  
            <column  name="emplastnameDemo"  type="varchar(50)">  
                <constraints  nullable="false"/>  
            </column>  
            <column  name="empstate"  type="char(2)"/>  
        </createTable>  
    </changeSet> 

